                <script>
            
            function getLocation() {

                if($('.Btn--bookdemo').length > 0 || $('.Btn--bookdemo-enterprise').length > 0 || $('.js-reviews-contact-num').length > 0 || $('.js-price-free').length > 0 || $('.js-price-starter').length > 0 || $('.js-price-growing').length > 0 || $('.js-trust-img-src').length > 0) {

                    $.post("/get-location", function(resp){
                        var $startUpPrice = '45';
                        if(resp.currency == 'AU$') {
                            $startUpPrice = '65';
                        } else if(resp.currency == 'EUâ‚¬') {
                            $startUpPrice = '50';
                        }

                        if (resp.enterprisePrice) {
                            $enterprisePrice = resp.enterprisePrice+"+";
                        } else {
                            $enterprisePrice = "799+";
                        }



                        $('.Btn--bookdemo-new').attr('href',resp.demoUrl);
                        $('.Btn--bookdemo-enterprise-new').attr('href',resp.enterpriseDemoUrl);
                        $('.js-reviews-contact-num').html(resp.contactNum);
                        $('.js-price-free').html(resp.currency+resp.price['free']);
                        $('.js-price-essential').html(resp.currency+resp.price['essential']);
                        $('.js-price-starter').html(resp.currency+resp.price['basic']);
                        $('.js-price-growing').html(resp.currency+resp.price['business']);
                        $('.js-price-enterprise').html(resp.currency+resp.price['enterprise']);
                        $('.js-price-tax').html(resp.purchaseTax);
                        $('.js-price-start-up').html(resp.currency+$startUpPrice);
                        $('.js-currencySymbol').html(resp.currency+$enterprisePrice);

                        if (resp.translateImg !== '--de') {
                            document.querySelector('.js-impressum-link').style.display='none';
                        }

                        if ($('.js-trust-img-src').attr('src')) {
                            $('.js-trust-img-src').attr('src', $('.js-trust-img-src').attr('src').replace('--uk--', resp.trustusImg));
                            $('.js-trust-img-srcset-webp-mob').attr('srcset', $('.js-trust-img-srcset-webp-mob').attr('srcset').replace('--uk--', resp.trustusImg));
                            $('.js-trust-img-srcset-png-mob').attr('srcset', $('.js-trust-img-srcset-png-mob').attr('srcset').replace('--uk--', resp.trustusImg));
                            $('.js-trust-img-srcset-webp-desk').attr('srcset', $('.js-trust-img-srcset-webp-desk').attr('srcset').split('--uk--').join(resp.trustusImg));
                            $('.js-trust-img-srcset-png-desk').attr('srcset', $('.js-trust-img-srcset-png-desk').attr('srcset').split('--uk--').join(resp.trustusImg));
                        }

                        if ($('.js-translate-image-src').attr('src')) {
                            $('.js-translate-image-src').attr('src', $('.js-translate-image-src').attr('src').replace('--en', resp.translateImg));
                            $('.js-translate-image-srcset-webp-mob').attr('srcset', $('.js-translate-image-srcset-webp-mob').attr('srcset').replace('--en', resp.translateImg));
                            $('.js-translate-image-srcset-png-mob').attr('srcset', $('.js-translate-image-srcset-png-mob').attr('srcset').replace('--en', resp.translateImg));
                            $('.js-translate-image-srcset-webp-desk').attr('srcset', $('.js-translate-image-srcset-webp-desk').attr('srcset').split('--en').join(resp.translateImg));
                            $('.js-translate-image-srcset-png-desk').attr('srcset', $('.js-translate-image-srcset-png-desk').attr('srcset').split('--en').join(resp.translateImg));
                        }
                    });
                }
            }

            $( document ).ready(function() {
                getLocation();
            });
        </script>
        
