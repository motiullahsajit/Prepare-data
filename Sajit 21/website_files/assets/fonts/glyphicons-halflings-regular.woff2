<script>
	var _submitting = false;

	$('#input-email').tooltip({
	    placement: "top",
	    trigger: "foucs"
	});

	$('input[name="company-name"]').keyup(function(){
		$(this).parents('.RegistrationModule').find('.result-company-name').html($(this).val());
	});

	$('input[name="url"]').keyup(function(){
		$(this).parents('.RegistrationModule').find('.result-url').html($(this).val());
	});

	function validateEmail(email) {
	  var re = /^(([^<>()[\]\\.,;:\s@\"]+(\.[^<>()[\]\\.,;:\s@\"]+)*)|(\".+\"))@((\[[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}\])|(([a-zA-Z\-0-9]+\.)+[a-zA-Z]{2,}))$/;
	  return re.test(email);
	}

	function validatePhone(phone) {
		var re = /[0-9\s\+\-\(\)]{6,}/;
		var re_us = /^(?:\+1[-.\s]?)?(?:\(?\d{3}\)?[-.\s]?)?\d{3}[-.\s]?\d{4}$/;
		return re.test(phone) || re_us.test(phone);
	}

	function validateUrl(url) {
		var re = /(?:https?:\/\/)?[-a-zA-Z0-9@:%._\+~#=]{2,256}\.[a-z]{2,6}/;
		return re.test(url);

	}

	function validationErrors(id) {
		var errors = 0;
		var input = false;

		window.formId = '#form-id-' + id + ' ';

		$(formId + ' .RM__cell').each(function (i, el) {
			input = $('input[required]', el);
			if (input.length) {
				if ($(input).attr('name') === 'url' && !validateUrl($(input).val())) {
					errors++;
					$('.inputErrorMsg', el).addClass('isActive');

				} else if ($(input).attr('type') === 'text' && $(input).val().length < 3) {
					errors++;
					$('.inputErrorMsg', el).addClass('isActive');

				} else if ($(input).attr('type') === 'email' && !validateEmail($(input).val())) {
					errors++;
					$('.inputErrorMsg', el).addClass('isActive');

				} else if ($(input).attr('type') === 'tel' && !validatePhone($(input).val())) {
					errors++;
					$('.inputErrorMsg', el).addClass('isActive');
				} else {
					$('.inputErrorMsg', el).removeClass('isActive');
				}
			}
		});

		return errors;
	}


	function signUpFreeSubmit(id)
	{
		if (_submitting || validationErrors(id)) {
			return;
		}

		window.formId = '#form-id-'+id+ ' ';
		_submitting = true;

		$(formId+'#signUpFreeSubmitButton').attr('disabled', 'true').val('Processing...');
		$(formId+'.inputErrorMsg').removeClass('isActive');
		var freeSignupData = {
			name: $(formId+'#input-name').val(),
			email: $(formId+'#input-email').val(),
			phone: $(formId+'#input-phone').val(),
			role: $(formId+'#input-role').val(),
			'company-name': $(formId+'#input-company-name').val(),
			url: $(formId+'#input-url').val(),
			pricePlan: $(formId+'input[name="price_plan"]').val(),
			price_plan_id: $(formId+'input[name="price_plan_id"]').val(),
			notes: $(formId+'input[name="notes"]').val(),
			signup_journey: $(formId+'input[name="signup_journey"]').val(),
			client_id: $(formId+'input[name="client_id"]').val(),
			signup_route: $(formId+'#signup-route').val(),
			signup_page_url: $(formId+'input[name="signup_page_url"]').val(),
			signup_requested_invites: $(formId+'input[name="signup_requested_invites"]').val(),
			signup_price: $(formId+'input[name="signup_price"]').val(),
			signup_locations: $(formId+'input[name="signup_locations"]').val()
		};

		var emailDomain = freeSignupData.email.split('@')[1];

		if(freeSignupData.url.indexOf(emailDomain) === -1){
			freeSignupData.signup_route = 'free-account-signup-no-email-match';
		}
		var formData = new FormData;
		for (var key in freeSignupData) {
			formData.append(key, freeSignupData[key]);
		}

		if ($('#signupPopupCompetitor').is(':visible')) {
			formData.append('contract', document.getElementById('input-contract-file').files[0]);
		}

		$.ajax({
            url: '/signup/validate-email',
            data: formData,
            type: 'POST',
			processData: false,
		    contentType: false,
            success: function(resp) {
				_submitting = false;

				let email_element = $('.RM__cell--email .inputErrorMsg');
				let url_element   = $('.RM__cell--url .inputErrorMsg');

				if (resp.success) {
					if (resp.activated) {
						$(formId + '.signup-success').addClass('isActive');
					} else {
						$(formId + '.signup-success--no-match').addClass('isActive');
					}

					return false;
				} else {
					switch (resp.message) {
						case 'email-and-url':
							email_element.text('You already have a REVIEWS.io account').addClass('isActive');
							url_element.text('The website URL is not valid').addClass('isActive');
							break;
						case 'email-only':
							email_element.text('You already have a REVIEWS.io account').addClass('isActive');
							url_element.removeClass('isActive');
							break;
						case 'url-only':
							url_element.text('The website URL is not valid').addClass('isActive');
							email_element.removeClass('isActive');
							break;
						case 'email-free':
							email_element.text('The email address is not valid').addClass('isActive');
							url_element.removeClass('isActive');
							break;
						case 'url-exists':
							url_element.text('You already have a REVIEWS.io account').addClass('isActive');
							email_element.removeClass('isActive');
							break;
					}

					_submitting = false;

					$(formId + '#signUpFreeSubmitButton').prop('disabled', false).val('Complete');
				}
            },
            error: function(data) {
				_submitting = false;

            	if(!!data.responseJSON){
	            	if(!!data.responseJSON){
	            		for( key in data.responseJSON) {
	            			if(key == 'companyName'){
	            				var inputField = 'input[name=company-name]';
	            			}

	            			if(key == 'role'){
	            				var inputField = '#input-role';
	            			}

	            			if(key !== 'companyName' && key !== 'url' && key !== 'role'){

	            				var inputField = 'input[name="'+ key + '"]';
	            			}

	            			$(formId+inputField).prev().addClass('isActive');

	            			if(key === 'url'){

	            				$(formId+'.invalid-url').addClass('isActive');
	            			}
	            		}
	            	}
	            	if(!!data.responseJSON.mesg && data.responseJSON.mesg === 'client_exists'){

						$(formId+'.inputErrorMsg.client-exists').addClass('isActive');
						$(formId+'.inputErrorMsg.client-exists').html("Access cannot be granted as " + data.responseJSON.host + " account already exists.");

	            	}
	            	if(!!data.responseJSON.mesg && data.responseJSON.mesg === 'invalid_url'){

						$(formId+'.inputErrorMsg.invalid-url').addClass('isActive');
	            	}

	            	if(!!data.responseJSON.mesg && data.responseJSON.mesg === 'free_email'){

						$(formId+'.inputErrorMsg.free-email').addClass('isActive');
	            	}

	            	if(!!data.responseJSON.mesg && data.responseJSON.mesg === 'customer_care'){

						$(formId+'.inputErrorMsg.customer-care').addClass('isActive');
	            	}

	            	if(!!data.responseJSON.mesg && data.responseJSON.mesg === 'reviews_websites'){

						$('.inputErrorMsg.reviews-websites').addClass('isActive');
	            	}

	            	if(!!data.responseJSON.mesg && data.responseJSON.mesg === 'match_email'){

						$(formId+'.inputErrorMsg.match_email').addClass('isActive');

	            	}
	            }
                $(formId+'#signUpFreeSubmitButton').removeAttr('disabled').val('Complete');
            }
        });

	}

</script>
    </div>
</div>


