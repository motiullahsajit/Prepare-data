<script>
    $(".js-open-signuppopupalt").click(function(){
        $(".SignupPopupAlt").addClass("isOpen");
        if (window.innerWidth < 640){
            document.body.style.overflow = "hidden";
            document.body.style.width = "100%";
            document.body.style.position = "fixed";
        }
    });

    $(".SignupPopupAlt__backdrop, .js-close-signuppopupalt").click(function(){
        $(".SignupPopupAlt").removeClass("isOpen");

        if (window.innerWidth < 640){
            document.body.style.overflow = "visible";
            document.body.style.width = "auto";
            document.body.style.position = "static";
        }
    });



    //Detect page and assign a signup route and signup page url based on specific page url
    var currentURL = window.location.pathname;
    var splitUrl = currentURL.split('/');
    var currentPageName = splitUrl[splitUrl.length-1];

    switch (currentPageName) {
        case 'company-reviews':
            $("input[name='signup_route']").val('company');
            $("input[name='signup_page_url']").val(window.location.href);
            break;
        case 'productreviews':
            $("input[name='signup_route']").val('product');
            $("input[name='signup_page_url']").val(window.location.href);
            break;
        case 'local-reviews':
            $("input[name='signup_route']").val('local');
            $("input[name='signup_page_url']").val(window.location.href);
            break;
        default:
            $("input[name='signup_page_url']").val(window.location.href);
    }

</script>
